#include <stdio.h>
int main(){
    int a, b , product;
    a = 20;
    b = 7;
    
    //Calculating product
    product = a + b;
    printf("add=%d\n", product);
    product = a - b;
    printf("sub=%d\n", product);
    product = a * b;
    printf("Product=%d\n", product);
    product = a / b;
    printf("div=%d\n", product);
    product = a % b;
    printf("mod=%d\n", product);
    return 0;
}
